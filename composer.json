{
    "name": "upmind/enhance-sdk",
    "description": "Enhance.com PHP SDK automatically generated from the Enhance OpenAPI spec using OpenAPI Generator",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Harry Lewis",
            "email": "harry@upmind.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Upmind\\EnhanceSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Upmind\\EnhanceSdk\\Test\\": "test/"
        }
    }
}